{
  "name": "switchaccount",
  "version": "1.2.0",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run index.ts",
    "build": "mkdir -p build && bun build --compile --minify --sourcemap --outfile build/ghswitch ./index.ts",
    "build:linux": "mkdir -p build && bun build --compile --target=bun-linux-x64 --minify --sourcemap --outfile build/ghswitch ./index.ts",
    "build:linux-arm": "mkdir -p build && bun build --compile --target=bun-linux-arm64 --minify --sourcemap --outfile build/ghswitch-linux-arm64 ./index.ts",
    "build:windows": "mkdir -p build && bun build --compile --target=bun-windows-x64 --minify --sourcemap --outfile build/ghswitch.exe ./index.ts",
    "build:macos": "mkdir -p build && bun build --compile --target=bun-darwin-x64 --minify --sourcemap --outfile build/ghswitch-macos ./index.ts",
    "build:macos-arm": "mkdir -p build && bun build --compile --target=bun-darwin-arm64 --minify --sourcemap --outfile build/ghswitch-macos-arm64 ./index.ts",
    "build:all": "bun run build:linux && bun run build:linux-arm && bun run build:windows && bun run build:macos && bun run build:macos-arm",
    "release": "./release.sh",
    "release:draft": "./release.sh --draft",
    "clean": "rm -f build/* checksums.txt"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/figlet": "^1.7.0",
    "@types/prompts": "^2.4.9",
    "boxen": "^8.0.1",
    "chalk": "^5.6.0",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "kleur": "^4.1.5",
    "ora": "^8.2.0",
    "prompts": "^2.4.2"
  }
}
